{
  "name": "marp-editable-slides-sample",
  "version": "1.0.0",
  "description": "Marpの編集機能を試せるミニマルなサンプルリポジトリ",
  "scripts": {
    "build": "npx @marp-team/marp-cli slides.md --allow-local-files",
    "build:pdf": "npx @marp-team/marp-cli slides.md --pdf --allow-local-files",
    "build:pptx": "npx @marp-team/marp-cli slides.md --pptx --pptx-editable --allow-local-files",
    "watch": "npx @marp-team/marp-cli slides.md --watch --allow-local-files",
    "watch:pdf": "npx @marp-team/marp-cli slides.md --watch --pdf --allow-local-files",
    "watch:pptx": "npx @marp-team/marp-cli slides.md --watch --pptx --pptx-editable --allow-local-files"
  },
  "keywords": [
    "marp",
    "slides",
    "presentation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@marp-team/marp-cli": "^4.1.0",
    "@marp-team/marpit": "^2.6.1",
    "chokidar": "^3.5.3"
  }
}
